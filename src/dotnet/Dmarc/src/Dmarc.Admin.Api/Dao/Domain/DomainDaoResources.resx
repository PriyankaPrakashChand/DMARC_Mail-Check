  <data name="InsertDomain" xml:space="preserve">
    <value>INSERT INTO domain
(name,publish, created_by)
VALUES
(@name,b'1', @createdBy) ON DUPLICATE KEY UPDATE id=LAST_INSERT_ID(id), publish=b'1';</value>
  </data>
  <data name="SelectDomainById" xml:space="preserve">
    <value>SELECT id, name 
FROM domain
WHERE id = @id and publish=b'1';</value>
  </data>
  <data name="SelectDomainsByGroupId" xml:space="preserve">
    <value>SELECT d.id, d.name
FROM domain d
JOIN group_domain_mapping gdm ON d.id = gdm.domain_id
JOIN `group` g ON g.id = gdm.group_id
WHERE d.publish = b'1' 
AND g.id = @groupId
AND (@search IS NULL OR LOWER(d.name) LIKE CONCAT(LOWER(@search), '%'))
ORDER BY d.name
LIMIT @offset, @pageSize;</value>
  </data>
  <data name="SelectDomainsByName" xml:space="preserve">
    <value>SELECT 
d1.id,
d1.name
FROM
(

(SELECT 
d.id,
d.name
FROM domain d
WHERE d.publish=b'1' AND  (@search IS NULL OR LOWER(d.name) LIKE CONCAT('%', LOWER(@search), '%'))
ORDER BY d.name
LIMIT 0, @limit)

UNION

(SELECT 
d.id,
d.name
FROM domain d
WHERE d.publish=b'1' AND d.id IN ({0}))
) d1
ORDER by d1.name</value>
  </data>
  <data name="SelectDomainsByUserId" xml:space="preserve">
    <value>SELECT d.id, d.name
FROM domain d
JOIN group_domain_mapping gdm ON d.id = gdm.domain_id
JOIN `group` g ON g.id = gdm.group_id
JOIN group_user_mapping gum ON g.id = gum.group_id
JOIN user u on u.id = gum.user_id
WHERE d.publish=b'1' AND u.id = @userId
AND (@search IS NULL OR LOWER(d.name) LIKE CONCAT(LOWER(@search), '%'))
ORDER BY d.name
LIMIT @offset, @pageSize;</value>
